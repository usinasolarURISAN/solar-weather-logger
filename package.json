{
  "name": "web-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=test ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "migrations:run": "yarn typeorm migration:run",
    "migrations:revert": "yarn typeorm migration:revert",
    "test": "NODE_ENV=dev ts-node-dev --transpile-only --ignore-watch node_modules src/test.ts",
    "test:index": "NODE_ENV=dev ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "prettier": "prettier --ignore-path .prettierignore \"**/*.+(js|ts|json)\"",
    "eslint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "migrations:run:dev": "NODE_ENV=dev yarn typeorm migration:run",
    "migrations:run:hml": "NODE_ENV=hml yarn typeorm migration:run",
    "migrations:run:prd": "NODE_ENV=prd yarn typeorm migration:run",
    "build": "rm -fr build && rm -fr node_modules && yarn --frozen-lockfile && tsc && cp ./ecosystem.config.js build && cp ./package.json build && cp ./yarn.lock build && cp deploy.bash build",
    "update-build": "mkdir ~/repos/solar-releases/solar-weather-logger || rm -r ~/repos/solar-releases/solar-weather-logger/* && cp -r ./build/* ~/repos/solar-releases/solar-weather-logger/ && code ~/repos/solar-releases"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.0",
    "prettier": "^2.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tslint": "5.12.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "cron": "^1.8.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.6",
    "uuid": "^8.3.2"
  }
}
